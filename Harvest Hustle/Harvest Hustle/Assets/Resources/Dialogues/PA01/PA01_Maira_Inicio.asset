    Reward:
      Item: {fileID: 0}
      Amount: 0
    Quest:
      StartQuest: {fileID: 0}
  - Speaker: {fileID: 11400000, guid: 08a9e8cf8d2293144ae65c42855972a1, type: 2}
    Text: "O problema \xE9 que aqui comigo eu n\xE3o tenho mais nenhuma folha de
