    Reward:
      Item: {fileID: 0}
      Amount: 0
    Quest:
      StartQuest: {fileID: 0}
  - Speaker: {fileID: 11400000, guid: 08a9e8cf8d2293144ae65c42855972a1, type: 2}
    Text: "Voc\xEA poderia trazer pra mim?"
    TextSpeed: 1
    Reward:
      Item: {fileID: 0}
      Amount: 0
    Quest:
      StartQuest: {fileID: 0}
  - Speaker: {fileID: 11400000, guid: 08a9e8cf8d2293144ae65c42855972a1, type: 2}
    Text: "J\xE1 deve ter topado com essa planta por a\xED, ela virou uma erva daninha
